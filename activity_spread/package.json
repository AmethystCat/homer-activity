{
  "private": true,
  "scripts": {
    "start": "./node_modules/.bin/cross-env NODE_ENV=development ./node_modules/.bin/webpack-dev-server --progress --colors --content-base build --history-api-fallback --define process.env.NODE_ENV='\"development\"'",
    "deploy": "./node_modules/.bin/cross-env NODE_ENV=production ./node_modules/.bin/webpack --define process.env.NODE_ENV='\"production\"' -p",
    "test": "./node_modules/.bin/cross-env NODE_ENV=test ./node_modules/.bin/_mocha --compilers js:babel-core/register -r ./test/helper.js -R nyan"
  },
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-0"
    ],
    "env": {
      "development": {
        "plugins": [
          [
            "react-transform",
            {
              "transforms": [
                {
                  "transform": "react-transform-hmr",
                  "imports": [
                    "react"
                  ],
                  "locals": [
                    "module"
                  ]
                }
              ]
            }
          ]
        ]
      }
    }
  },
  "devDependencies": {
    "babel-core": "^6.9.1",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "cross-env": "^1.0.8",
    "css-loader": "^0.23.1",
    "enzyme": "^2.5.1",
    "eslint": "^2.2.0",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-react": "^5.1.1",
    "expose-loader": "^0.7.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "iscroll": "^5.2.0",
    "jquery": "^3.1.1",
    "jsdom": "^9.8.2",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "mocha": "^3.1.2",
    "react": "^15.1.0",
    "react-addons-css-transition-group": "^15.3.2",
    "react-addons-test-utils": "^15.3.2",
    "react-dom": "^15.1.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.8.1",
    "react-transform-hmr": "^1.0.4",
    "redux": "^3.5.2",
    "sinon": "^1.17.6",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "babel-plugin-transform-runtime": "^6.9.0"
  }
